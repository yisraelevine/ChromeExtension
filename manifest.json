{
    "manifest_version": 3,
    "name": "Daily Shiurim Fullscreen",
    "version": "1.3.0",
    "description": "Makes Daily Shiurim Fullscreen on click",
    "permissions": [],
    "content_scripts": [
        {
            "matches": [
                "https://he.chabad.org/dailystudy/*"
